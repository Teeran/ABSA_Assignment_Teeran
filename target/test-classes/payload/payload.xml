<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Document xmlns="urn:iso:std:iso:20022:tech:xsd:pain.008.001.03">
    <CstmrDrctDbtInitn>
        <GrpHdr>
            <MsgId>#(MsgId)</MsgId>
            <CreDtTm>#(CreDtTm)</CreDtTm>
            <NbOfTxs>#(NbOfTxs)</NbOfTxs>
            <CtrlSum>#(CtrlSum)</CtrlSum>
            <InitgPty>
                <Nm>#(InitgPtyNM)</Nm>
            </InitgPty>
        </GrpHdr>
        <PmtInf>
            <PmtInfId>#(PmtInfId )</PmtInfId>
            <PmtMtd>#(PmtMtd)</PmtMtd>
            <ReqdColltnDt>#(ReqdColltnDt)</ReqdColltnDt>
            <Cdtr>
                <Nm>#(CdtrNM)</Nm>
                <CtctDtls>
                    <PhneNb>#(PhneNb)</PhneNb>
                    <EmailAdr>#(EmailAdr)</EmailAdr>
                </CtctDtls>
            </Cdtr>
            <CdtrAcct>
                <Id>
                    <Othr>
                        <Id>#(CdtrAcctID)</Id>
                    </Othr>
                </Id>
            </CdtrAcct>
            <CdtrAgt>
                <FinInstnId>
                    <ClrSysMmbId>
                        <MmbId>#(MmbId)</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </CdtrAgt>
            <DrctDbtTxInf>
                <PmtId>
                    <EndToEndId>#(EndToEndId)</EndToEndId>
                </PmtId>
                <PmtTpInf>
                    <SvcLvl>
                        <Prtry>#(SvcLvlPrtry)</Prtry>
                    </SvcLvl>
                    <LclInstrm>
                        <Prtry>#(LclInstrmPrtry)</Prtry>
                    </LclInstrm>
                    <SeqTp>#(SeqTp)</SeqTp>
                    <CtgyPurp>
                        <Prtry>#(CtgyPurpPrtry)</Prtry>
                    </CtgyPurp>
                </PmtTpInf>
                <InstdAmt Ccy="ZAR">#(InstdAm)</InstdAmt>
                <ChrgBr>SLEV</ChrgBr>
                <DrctDbtTx>
                    <MndtRltdInf>
                        <MndtId>#(MndtId)</MndtId>
                    </MndtRltdInf>
                </DrctDbtTx>
                <DbtrAgt>
                    <FinInstnId>
                        <ClrSysMmbId>
                            <MmbId>#(MmbId)</MmbId>
                        </ClrSysMmbId>
                    </FinInstnId>
                </DbtrAgt>
                <Dbtr>
                    <Nm>#(DbtrNM)</Nm>
                </Dbtr>
                <DbtrAcct>
                    <Id>
                        <Othr>
                            <Id>#(DbtrAcctID)</Id>
                        </Othr>
                    </Id>
                    <Tp>
                        <Prtry>#(TPPrtry)</Prtry>
                    </Tp>
                </DbtrAcct>
                <RmtInf>
                    <Ustrd>#(Ustrd)</Ustrd>
                    <Strd>
                        <RfrdDocInf>
                            <RltdDt>#(RltdDt)</RltdDt>
                        </RfrdDocInf>
                    </Strd>
                </RmtInf>
            </DrctDbtTxInf>
        </PmtInf>
    </CstmrDrctDbtInitn>
</Document>